    "**/*.tsx"
, "pages/index.jsx", "components/About/Initial.jsx", "components/About/Reading.jsx", "components/Projects/CustomEditor.jsx", "components/About/Diplomas.jsx", "scripts/isMobile.js"  ],
  "exclude": [
